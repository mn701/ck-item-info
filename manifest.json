{
  "name": "ck online shop item info",
  "version": "1.01",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.1.0.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_title": "ck"
  },
  "background": {
    "scripts" : ["jquery-3.1.0.min.js", "background.js"]
  },
  "permissions": ["tabs"]
}
